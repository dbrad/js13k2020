  "scripts": {
    "compile": "tsc && gulp build",
    "delete-dist": "yarn rimraf ./dist",
    "create-dist": "yarn mkdirp ./dist/src",
    "inline": "html-inline -i ./build/release/www/index.html -o ./dist/src/index.html",
    "build": "yarn run delete-dist && yarn run create-dist && yarn run compile && yarn run inline",
    "zip": "yarn run build -s && yarn node ./scripts/zip-file.js -s && yarn size -s",
    "size": "yarn node ./scripts/check-file-size.js",
    "submit": "yarn run zip"
  },
  "dependencies": {
    "advzip-bin": "^2.0.0",
    "archiver": "^4.0.1",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-json-minify": "^1.2.2",
    "gulp-minify-html": "^1.0.6",
    "gulp-preprocess": "^3.0.3",
    "gulp-sourcemaps": "^2.6.5",
    "html-inline": "^1.2.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "transfob": "^1.0.0",
    "typescript": "^3.9.5"
  }
}